<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <!-- Define the package sources -->
  <packageSources>
    <clear />
    <!-- 'key' can be any identifier for your sources -->
    <add key="nuget.org" value="https://api.nuget.org/v3/index.json" />
  </packageSources>

  <!-- Define mappings by adding pacakge patterns beneath the target source. -->
  <packageSourceMapping>
    <!-- key value for <pacakgeSource> should match key values from <packageSources> element -->
    <packageSource key="nuget.org">
      <package pattern="*" />
    </packageSource>
  </packageSourceMapping>
</configuration>
